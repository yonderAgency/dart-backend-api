                                                                            >
                                                                                <%
                                                                                var
                                                                                dt
                                                                                =
                                                                                new
                                                                                Date();
                                                                                var
                                                                                year
                                                                                =
                                                                                dt.getFullYear();
                                                                                %>
                                                                                Copyright
                                                                                <%=year%>
                                                                                <%=sails.config.dynamics.APPLICATION_NAME%>
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </center>
    </body>
